{
  "css.validate": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.tabSize": 2,
  "git.pruneOnFetch": true,
  "git.rebaseWhenSync": true,
  "cSpell.ignorePaths": [
    "**/package.json",
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".vscode-insiders",
    ".vscode-server",
    "*.config.js"
  ],
  "references.preferredLocation": "view",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    ".next": true,
    ".yarn": true
  },
  "cSpell.words": [
    "automerge",
    "chakra",
    "ervwalter",
    "highcharts",
    "Highstock",
    "ohlc",
    "resetpassword"
  ],
  "dotnet.defaultSolution": "apps/api/TrendWeight.sln"
}

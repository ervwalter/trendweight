{
  "extends": [
    "config:js-app",
    ":dependencyDashboard",
    ":prHourlyLimitNone",
    ":prConcurrentLimitNone"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "matchUpdateTypes": ["minor", "patch"],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch"
    }
  ],
  "lockFileMaintenance": {
    "enabled": true
  }
}
